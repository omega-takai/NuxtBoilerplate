{
  "name": "nuxt-boilerplate",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "pregenerate": "yarn clean",
    "generate": "nuxt generate",
    "lint:format": "prettier -c . --ignore-path .gitignore",
    "lint:script": "eslint --ext \".js,.ts,.vue\" --max-warnings 0 --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "clean": "rimraf .nuxt dist .out public coverage test/coverage && sort-package-json",
    "lint:fix": "yarn lint:format --write && yarn lint:script --fix && yarn lint:style --fix",
    "test": "jest --config test/jest.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": "prettier -c --write",
    "*.{js,.ts,vue}": "eslint --fix --max-warnings 0 --no-ignore",
    "*.{css,.scss,vue}": "stylelint --fix"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/vue": "^6.3.7",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "babel-loader": "^8.2.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "fibers": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "sort-package-json": "^1.50.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^27.0.3",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.9.8"
  }
}
